    <body>
       <header>
      <nav class="navbar navbar-expand-lg navbar-dark bg-dark">
  <a class="navbar-brand" href="#">DealGames</a>
  <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
    <span class="navbar-toggler-icon"></span>
  </button>

  <div class="collapse navbar-collapse" id="navbarSupportedContent">
    <ul class="navbar-nav mr-auto">
      <li class="nav-item active">
        <a class="nav-link" href="/">Accueil</a>
      </li>
      <li class="nav-item">
        <a class="nav-link" href="#">Annonces</a>
      </li>
      {% if app.user %}
      <form action="/annonce/create" method="GET">
        <button class="navbar-left btn btn-outline-sucess" type="submit"> + Déposer une annonce</button>
      </form>
      {% endif %}
      <li class="nav-item">
        <a class="nav-link" href="/login">Compte</a>
      </li>
	  {% if app.user %}
	  <li class="nav-item">
        <a class"nav-link" href="{{ path('app_logout') }}">Deconnexion</a>
      </li>
	  {% endif %}
    </ul>
    
  </div>
</nav>
    </header>

